/* component/searchbar-return/searchbar-return.wxss */
view {
    box-sizing: border-box;
    overflow: hidden;
  }
  .searchbar {
    background-color: #11D183; 
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
  }
  .return {
    padding: 0 10rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
  }
  .searchbar-wrapper{
      display: flex;
      flex-direction: row;
  }
  .returnImage{
    height: 32rpx;
    width: 32rpx;
  }
  .search-group {
    width: 100%;

    height: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    border: 1px solid #666;
    border-radius: 30rpx;
    padding: 10rpx;
    margin-left: 15rpx;

  }
  .search-input {
    font-size: 25rpx;
    margin-left: 8px;
    margin-right: 28px;
  }
 .searchicon{
    height: 32rpx;
    width: 32rpx;
    margin-left:20rpx
 }
  .delete-clickicon{
    height: 32rpx;
    width: 32rpx;
  }